{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2021", "DOM"],
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "useUnknownInCatchVariables": false,
    "paths": {
      "selenium-webdriver/chrome": ["./node_modules/selenium-webdriver/lib/chrome"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "CloudMusic_Auto_Player"
  ]
}
